        "jquery.cookie": "^1.4.1",
        "laravel-mix": "^5.0.4",
        "laravel-mix-polyfill": "^2.0.0",
        "magnific-popup": "^1.1.0",
        "nouislider": "^14.2.0",
        "object-fit-images": "^3.2.4",
        "prismjs": "^1.20.0",
        "smooth-scroll": "^16.1.3",
        "swiper": "^5.3.7"
    },
    "dependencies": {
        "flatpickr": "^4.6.3",
